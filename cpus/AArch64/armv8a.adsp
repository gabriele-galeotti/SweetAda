      nTWI       : Boolean;      -- Traps execution of WFI instructions at EL0 to EL2, from both Execution states.
      Reserved2  : Bits_1  := 0;
      nTWE       : Boolean;      -- Traps execution of WFE instructions at EL0 to EL2, from both Execution states.
