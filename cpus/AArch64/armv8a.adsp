   FWB_NONE : constant := 2#0000#; -- HCR_EL2.FWB bit is not supported.
   FWB_YES  : constant := 2#0001#; -- HCR_EL2.FWB is supported.

   TTL_NONE : constant := 2#0000#; -- TLB maintenance instructions by address have bits[47:44] as RES0.
