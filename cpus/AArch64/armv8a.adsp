      RV        : Boolean;      -- Execution state control for lower Exception levels
      CD        : Boolean;      -- Stage 2 Data access cacheability disable.
      ID        : Boolean;      -- Stage 2 Instruction access cacheability disable.
      E2H       : Boolean;      -- EL2 Host.
